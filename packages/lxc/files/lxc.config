
config 'lxc' 'default'
       option 'path' '/var/lib/lxc'

       option 'network0_link' 'lxc0'
       option 'network0_name' 'eth0'
       option 'network0_peer' 'eth0'

       option 'network1_link' 'lxc1'
       option 'network1_name' 'eth1'
       option 'network1_peer' 'eth1'


config 'container' 'cn001'
        option 'name' 'cn001'
	option 'boot' '0'


